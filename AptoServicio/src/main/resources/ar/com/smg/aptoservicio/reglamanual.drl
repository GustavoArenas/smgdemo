package ar.com.smg.aptoservicio;

import ar.com.smg.aptoservicio.Response;

rule "reglamanual_1"
	ruleflow-group "AptoServicio-group"
	when
		person : Person(age >= 18, age <= 24, gender == "M", percentil >= 0, percentil <= 50)
	then
	    Response response = new Response();
		response.setType(true);
		response.setMessage("Esta OK");
		update(response);
		System.out.println("Entro por la regla ok");
end

