package ar.com.smg.aptoservicio;

rule "VIPT_0"
	when 
		person : Person(name : name == "Maradona")
	then 
		Response $var0 = new Response();
		$var0.setType( true );
		insert( $var0 );
end
rule "VIPT_1"
	when 
		person : Person(name : name != "Maradona")
	then 
		Response $var0 = new Response();
		$var0.setType( false );
		insert( $var0 );
end
